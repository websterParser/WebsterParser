    "@types/node": "^18.6.5",
    "@types/node-dir": "0.0.34",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.35.0"
